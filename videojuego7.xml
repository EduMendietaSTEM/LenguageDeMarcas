<?xml version="1.0" encoding="UTF-8"?>
<!--Crea un fichero XML con XSD que cumpla los siguientes requisitos:
#Elemento raíz videojuegos
#videojuegos está compuesto de elementos videojuego
#videojuego está compuesto por nombre, descripción, fecha de lanzamiento (día, mes y año), 
#nombre equipo desarrollo y nombre de empresa distribuidora.
#Además, videojuego tiene una lista géneros
#géneros debe tener uno o más elementos género.
#género está compuesto por nombre y descripción
#La descripción de los géneros es opcional
#La fecha de lanzamiento puede tener solo año (sin día ni mes)
#Debe haber al menos un videojuego.-->
<!-- <!DOCTYPE videojuegos [
    <!ELEMENT videojuegos (videojuego+)>
    <!ELEMENT videojuego ((nombre,descripcion, fechaDeLanzamiento, generos+))>
    <!ELEMENT nombre (#PCDATA)>
    <!ELEMENT descripcion (#PCDATA)>
    <!ELEMENT fechaDeLanzamiento (dia, mes, anio)>
    <!ELEMENT dia (#PCDATA)>
    <!ELEMENT mes (#PCDATA)>
    <!ELEMENT anio (#PCDATA)>
    <!ELEMENT generos (genero)>
    <!ELEMENT genero (Nombre, Descripcion)>
    <!ELEMENT Nombre (#PCDATA)>
    <!ELEMENT Descripcion (#PCDATA)>
]>-->
<videojuegos xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:noNamespaceSchemaLocation="./videojuegos7.xsd">
   <videojuego>
      <nombre>Kingdom Hearts</nombre>
      <descripcion>Matar sincorazones</descripcion>
      <fechaDeLanzamiento>
        <dia>14</dia>
        <mes>03</mes>
        <anio>2013</anio>
      </fechaDeLanzamiento>
      <generos>
        <genero>
          <Nombre></Nombre>
          <Descripcion></Descripcion>
          </genero>
        </generos>
   </videojuego>
</videojuegos>
